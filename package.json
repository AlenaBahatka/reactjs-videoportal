{
  "name": "reactjs-videoportal",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/cypress/**",
      "!src/index.jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageDirectory": "output/coverage/jest"
  },
  "version": "1.0.0",
  "description": "Create ReactJS small app",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.4",
    "node-fetch": "^2.5.0",
    "npm-run-all": "^4.1.5",
    "query-string": "^6.4.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "serialize-javascript": "^1.7.0",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "jest": "^24.5.0",
    "nodemon": "^1.19.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.3",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build:dev": "set NODE_ENV=development && webpack --mode=development --progress --colors",
    "build:prod": "set NODE_ENV=production && webpack --mode=production --progress --colors",
    "start": "webpack-dev-server --mode=development",
    "test": "jest --watch",
    "cypress:open": "cypress open",
    "dev:build-server-client": "set NODE_ENV=development && webpack --mode=development --config ./webpack --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlenaBahatka/reactjs-videoportal.git"
  },
  "author": "Alena",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AlenaBahatka/reactjs-videoportal/issues"
  },
  "homepage": "https://github.com/AlenaBahatka/reactjs-videoportal#readme"
}
